<table>
  <thead>
    <tr>
      <th>1</th>
      <th>2</th>
      <th>3</th>
      <th>4</th>
      <th>5</th>
    </tr>
  </thead>
  <tbody>
    <% @sheets.reverse.each_slice(5) do |row| %>
      <tr>
        <% row.each do |sheet| %>
          <td><%= "#{sheet.row}-#{sheet.column}" %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
